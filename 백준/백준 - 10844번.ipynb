    "n = int(input())\n",
    "\n",
    "dp = [0] *(n+1)\n",
    "\n",
    "if n == 1 :\n",
    "    print(9)\n",
    "elif n == 2 :\n",
    "    print(17)\n",
    "elif n > 2 :\n",
    "    \n",
    "    dp[1] = 9\n",
    "    dp[2] = 17\n",
    "\n",
    "    for i in range(3,n+1):\n",
    "        dp[i] = (dp[i-1] - i) * 2 + i\n",
    "\n",
    "    print(dp[n])"
