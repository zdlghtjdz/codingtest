    "S = input()\n",
    "\n",
    "sum_ = 0\n",
    "char_s = []\n",
    "for i in range(len(S)) :\n",
    "    #print(int(i))\n",
    "    if S[i].isdigit() : \n",
    "        sum_ += int(S[i])\n",
    "    else :\n",
    "        char_s.append(S[i])\n",
    "#print(sum_)\n",
    "char_s.sort()\n",
    "answer = ''.join(char_s)\n",
    "answer += str(sum_)\n",
    "print(answer)"
   ]
