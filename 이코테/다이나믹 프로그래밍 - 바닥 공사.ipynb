   "source": [
    "# 2 X N개의 바닥을 채우자.\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "# an = an-1 + an-2 * 2\n",
    "\n",
    "table = [0] * n\n",
    "table[0] = 1\n",
    "table[1] = 3\n",
    "\n",
    "for i in range(2,n):\n",
    "    table[i] = table[i-1] + table[i-2] * 2\n",
    "\n",
    "print(table[n-1] % 796796)"
   ]
